{
	"name": "ynn-ms-token",
	"description": "",
	"repository": "https://github.com/ynnjs/ynn-ms-token.git",
	"version": "0.0.0",
	"keywords": [
		"koa",
		"ynn",
		"jwt",
		"token",
		"code"
