	},
	"devDependencies": {
		"@lvchengbin/sequence": "^1.1.8",
		"@lvchengbin/sleep": "0.0.2",
		"eslint": "^5.8.0",
		"jest": "^23.6.0",
		"nodemon": "^1.18.6",
		"pre-commit": "^1.2.2",
		"supertest": "^3.1.0"
	},
	"license": "MIT",
	"dependencies": {
		"@lvchengbin/is": "0.0.24",
		"jsonwebtoken": "^8.3.0",
		"randomstring": "^1.1.5",
		"ynn": "0.0.13",
		"ynn-plugin-redis": "git+https://github.com/ynnjs/ynn-plugin-redis.git"
	},
	"scripts": {
		"start": "nodemon src",
		"debug": "nodemon src --debugging=true",
		"test": "jest --forceExit",
		"test-watch": "jest --watch",
		"test-cov": "jest --coverage --runInBand --forceExit --detectOpenHandles",
